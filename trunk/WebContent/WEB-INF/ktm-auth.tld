<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
                        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
 <tlib-version>1.3</tlib-version>
 <jsp-version>1.2</jsp-version>
 <short-name>auth</short-name>
 <uri>http://sarachai.com/tags-auth</uri>
 <description>
        
        ITSCI Authentication Library
        
    </description>
 <tag>
  <name>isUserInRole</name>
  <tag-class>org.ktm.tag.auth.IsUserInRoleTag</tag-class>
  <body-content>JSP</body-content>
  <description>
            
            Checks the user role as defined by the model element securityRole
            
        </description>
  <attribute>
   <name>role</name>
   <required>true</required>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
 </tag>
 <tag>
  <name>isUserNotInRole</name>
  <tag-class>org.ktm.tag.auth.IsUserNotInRoleTag</tag-class>
  <body-content>JSP</body-content>
  <description>
            
            Checks the user role as defined by the model element securityRole
            
        </description>
  <attribute>
   <name>role</name>
   <required>true</required>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
 </tag>
 <tag>
  <name>isUserInRoles</name>
  <tag-class>org.ktm.tag.auth.IsUserInRolesTag</tag-class>
  <body-content>JSP</body-content>
  <description>
            
            Checks the user role as defined by the model element securityRole
            
        </description>
  <attribute>
   <name>roles</name>
   <required>true</required>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
 </tag>
 <tag>
  <name>isUserNotInRoles</name>
  <tag-class>org.ktm.tag.auth.IsUserNotInRolesTag</tag-class>
  <body-content>JSP</body-content>
