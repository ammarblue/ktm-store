 <tag>
  <name>redirectPage</name>
  <tag-class>org.ktm.tag.auth.RedirectTag</tag-class>
  <body-content>empty</body-content>
  <description>
            
            Redirect to specific page
            
        </description>
  <attribute>
   <name>page</name>
   <required>true</required>
   <rtexprvalue>false</rtexprvalue>
  </attribute>
 </tag>
 <tag>
  <name>ktmInfo</name>
  <tag-class>org.ktm.tag.KTMInfoTag</tag-class>
  <body-content>empty</body-content>
  <description>
            
            Display info by method
            
        </description>
  <attribute>
   <name>method</name>
   <required>true</required>
   <rtexprvalue>false</rtexprvalue>
  </attribute>
